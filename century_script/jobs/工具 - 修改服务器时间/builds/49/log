#!/bin/bash -e

export PATH=/usr/bin/curl:$PATH

conf_file=~/jenkins/server_config.yaml
yaml() {
    python -c "import yaml;print(yaml.safe_load(open('$1'))$2)"
}

server_port=$(yaml $conf_file "['$server_name']['port']")
echo $date_param
ts=$(date -d "$date_param" +%s)
echo $ts
echo $force

curl "localhost:$server_port/timestamp?timestamp=$ts&force=$force"
pkyq
